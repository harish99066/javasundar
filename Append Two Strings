import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
     Scanner s=new Scanner(System.in);
        String t1=s.nextLine();
        String t2=s.nextLine();
        StringBuffer b=new StringBuffer(t1);
        b.append(t2);
        System.out.println(b);
    }
}
