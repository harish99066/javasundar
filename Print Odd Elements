import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
     Scanner s =new Scanner(System.in);
        
        int N=s.nextInt();
        
        int[] arr=new int[N];
        
        
    for(int i=0;i<N;i++)
        {
            arr[i]=s.nextInt();
        }
    for(int i=0;i<N;i++)
    {
        
        if(arr[i]%2!=0)
        {
            System.out.print(arr[i]+" ");
        }
    }
    }
}
