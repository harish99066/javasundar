import java.io.*;
import java.util.*;

public class Solution {
    public int fact(int a,int n)
    {
        for(int i=1;i<=a;i++)
        {
            n=n*i;
        }
        return n;
    }
    public static void main(String[] args) {
      Solution b=new Solution();
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int n=1;
        int x=b.fact(a,n);
        System.out.println(x);
        
    }
}
