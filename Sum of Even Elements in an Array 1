import java.util.Scanner;
public class Solution {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int [] a=new int[100];
        int n;
        int d=0;
        n=s.nextInt();
        for(int i=0;i<n;i++){
            a[i]=s.nextInt();
            if(a[i]%2==0){
                d=d+a[i];}
        }
        System.out.println(d);
}}
